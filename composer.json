{
    "name": "mklemersson/weather-ballon",
    "description": "Small project for showing weather statistics",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "symfony/console": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "WbApp\\": "src/",
            "Test\\WbApp\\": "test/"
        },
        "classmap": ["test/"]
    },
    "scripts": {
        "test": "phpunit -c test/phpunit.xml",
        "test-dox": "phpunit -c test/phpunit.xml --testdox"
    }
}
